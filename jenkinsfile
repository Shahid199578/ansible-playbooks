pipeline {
    agent any
    stages{
        stage("Checkout") {
            steps {
                git branch:'master', url: 'https://github.com/Shahid199578/ansible-playbooks.git'
            }
        }
        stage("Run Ansible Playbook") {
            steps {
                sh 'ansible-playbook playbook.yaml'
            }
        }
    }
}